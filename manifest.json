{
  "manifest_version": 2,
  "name": "Duck Email Extension",
  "version": "1.0",
  "permissions": ["webRequest", "webRequestBlocking", "https://quack.duckduckgo.com/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
